<!DOCTYPE html>
<html>
<head>
@*<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title – приложение ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")*@
    <title>@Resources.Resource.title</title>
    <!-- for-mobile-apps -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Smart Shop Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript">
        addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
               function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- //for-mobile-apps -->
    <link href="~/Content/css/bootstrap.css" rel="stylesheet" type="text/css" media="all" />
    <!-- pignose css -->
    <link href="~/Content/css/pignose.layerslider.css" rel="stylesheet" type="text/css" media="all" />


    <!-- //pignose css -->
    <link href="~/Content/css/style.css?v=33111" rel="stylesheet" type="text/css" media="all" />
    <!-- js -->
    <script type="text/javascript" src="~/Content/js/jquery-2.1.4.min.js"></script>
    <!-- //js -->
    <!-- cart -->
    <script src="~/Content/js/simpleCart.js?v=1"></script>
    <!-- cart -->
    <!-- for bootstrap working -->
    <script type="text/javascript" src="~/Content/js/bootstrap-3.1.1.min.js"></script>
    <!-- //for bootstrap working -->
    <link href='//fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Lato:400,100,100italic,300,300italic,400italic,700,900,900italic,700italic' rel='stylesheet' type='text/css'>
    <script src="~/Content/js/jquery.easing.min.js"></script>

</head>
<body>
    <div class="header">
        <div class="container">
            <ul>
                <li><span class="glyphicon glyphicon-time" aria-hidden="true"></span>@Resources.Resource.deleviry</li>
                <li><span class="glyphicon glyphicon-shopping-cart" aria-hidden="true"></span>@Resources.Resource.allDeleviry</li>
                <li><span class="glyphicon glyphicon-envelope" aria-hidden="true"></span><a href="mailto:@Resources.Resource.email">@Resources.Resource.emailActual</a></li>
            </ul>
        </div>
    </div>
    <!-- //header -->
    <!-- header-bot -->
    <div class="header-bot">
        <div class="container">
            <div class="col-md-3 header-left">
                <h1><a href="~/Home/index"><img src="~/Content/images/logoS.jpg"></a></h1>
            </div>
            <div class="col-md-6 header-middle">
                <form>
                    <div class="search">
                        <input type="search" value="@Resources.Resource.search" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Search';}" required="">
                    </div>
                    <div class="section_room">
                        <select id="country" onchange="change_country(this.value)" class="frm-field required">
                            <option value="null">@Resources.Resource.allCategories</option>
                            <option value="null">@Resources.Resource.venders</option>
                            <option value="AX">@Resources.Resource.subCategories</option>
                        </select>
                    </div>
                    <div class="sear-sub">
                        <input type="submit" value=" ">
                    </div>
                    <div class="clearfix"></div>
                </form>
            </div>
            <div class="col-md-3 header-right footer-bottom">
                <ul>
                    <li>
                        <a href="#" class="use1" data-toggle="modal" data-target="#myModal4"><span>@Resources.Resource.login</span></a>

                    </li>
                    <li><a class="fb" href="#"></a></li>
                    <li><a class="twi" href="#"></a></li>
                    <li><a class="insta" href="#"></a></li>
                    <li><a class="you" href="#"></a></li>
                </ul>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
    <!-- //header-bot -->
    <!-- banner -->
    <div class="ban-top">
        <div class="container">
            <div class="top_nav_left">
                <nav class="navbar navbar-default">
                    <div class="container-fluid">
                        <!-- Brand and toggle get grouped for better mobile display -->
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                                <span class="sr-only">Toggle navigation</span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>
                        <!-- Collect the nav links, forms, and other content for toggling -->
                        <div class="collapse navbar-collapse menu--shylock" id="bs-example-navbar-collapse-1">
                            <ul class="nav navbar-nav menu__list">
                                <li class="active menu__item menu__item--current"><a class="menu__link" href="/home/index">@Resources.Resource.home<span class="sr-only">(current)</span></a></li>
                                <li class="dropdown menu__item">
                                    <a href="#" class="dropdown-toggle menu__link" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">@Resources.Resource.venders<span class="caret"></span></a>
                                    <ul class="dropdown-menu multi-column columns-3">
                                        <div class="row">
                                            <div class="col-sm-6 multi-gd-img1 multi-gd-text ">
                                                <a href="mens.html"><img src="~/Content/images/woo1.jpg?v=222" alt=" " /></a>
                                            </div>
                                            <div class="col-sm-3 multi-gd-img">
                                                <ul class="multi-column-dropdown">
                                                    <li><a href="/home/venders/1">@Resources.Resource.adobe</a></li>
                                                    <li><a href="/home/venders/2">@Resources.Resource.atlassian</a></li>
                                                    <li><a href="/home/venders/3">@Resources.Resource.avast</a></li>
                                                    <li><a href="/home/venders/4">@Resources.Resource.esset</a></li>
                                                </ul>
                                            </div>
                                            <div class="col-sm-3 multi-gd-img">
                                                <ul class="multi-column-dropdown">
                                                   
                                                    <li><a href="/home/venders/9">@Resources.Resource.mcaffee</a></li>
                                                    <li><a href="/home/venders/6">@Resources.Resource.microsoft</a></li>
                                                    <li><a href="/home/venders/7">@Resources.Resource.sketchUp</a></li>
                                                    <li><a href="/home/venders/8">@Resources.Resource.zwsoft</a></li>
                                                </ul>
                                            </div>
                                            <div class="clearfix"></div>
                                        </div>
                                    </ul>
                                </li>
                                <li class="dropdown menu__item">
                                    <a href="#" class="dropdown-toggle menu__link" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">@Resources.Resource.categories<span class="caret"></span></a>
                                    <ul class="dropdown-menu multi-column columns-3">
                                        <div class="row">
                                            <div class="col-sm-3 multi-gd-img">
                                                <ul class="multi-column-dropdown">
                                                    <li><a href="/home/categories/2">@Resources.Resource.os</a></li>
                                                    <li><a href="/home/categories/4">@Resources.Resource.serviceSystems</a></li>
                                                    <li><a href="/home/categories/6">@Resources.Resource.DBMS</a></li>
                                                    <li><a href="/home/categories/5">@Resources.Resource.textEditors</a></li>
                                                </ul>
                                            </div>
                                            <div class="col-sm-3 multi-gd-img">
                                                <ul class="multi-column-dropdown">
                                                    
                                                    <li><a href="/home/categories/1">@Resources.Resource.officeSystems</a></li>
                                                    <li><a href="/home/categories/7">@Resources.Resource.engineering</a></li>
                                                    <li><a href="/home/categories/3">@Resources.Resource.programmingSystems</a></li>

                                                </ul>
                                            </div>
                                            <div class="col-sm-6 multi-gd-img multi-gd-text ">
                                                <a href="womens.html"><img src="~/Content/images/woo.jpg?v=222" alt=" " /></a>
                                            </div>
                                            <div class="clearfix"></div>
                                        </div>
                                    </ul>
                                </li>
                                @*<li class=" menu__item"><a class="menu__link" href="electronics.html">Electronics</a></li>
                                <li class=" menu__item"><a class="menu__link" href="codes.html">Short Codes</a></li>
                                <li class=" menu__item"><a class="menu__link" href="contact.html">contact</a></li>*@
                            </ul>
                        </div>
                    </div>
                </nav>
            </div>
            <div class="top_nav_right">
                <div class="cart box_1">
                    <a href="~/home/checkout">
                        <h3>
                            <div class="total">
                                <i class="glyphicon glyphicon-shopping-cart" aria-hidden="true"></i>
                                <span class="simpleCart_total"></span> (<span id="simpleCart_quantity" class="simpleCart_quantity"></span> @Resources.Resource.items)
                            </div>

                        </h3>
                    </a>
                    <p><a href="javascript:;" class="simpleCart_empty">@Resources.Resource.emptyCart</a></p>

                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
    <!-- //banner-top -->
        @RenderBody()
    <!-- footer -->
    <div class="footer">
        <div class="container">
            <div class="col-md-3 footer-left">
                <h2><a href="/home/index"><img src="~/Content/images/logoS.jpg" alt=" " /></a></h2>
                <p>@Resources.Resource.description
                </p>
            </div>
            <div class="col-md-9 footer-right">
                <div class="col-sm-6 newsleft">
                    <h3>@Resources.Resource.subscription</h3>
                </div>
                <div class="col-sm-6 newsright">
                    <form>
                        <input type="text" value="@Resources.Resource.email" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = '@Resources.Resource.email';}" required="">
                        <input type="submit" value="@Resources.Resource.submit">
                    </form>
                </div>
                <div class="clearfix"></div>
                <div class="sign-grds">
                    <div class="col-md-4 sign-gd">
                        <h4>@Resources.Resource.information</h4>
                        <ul>
                            <li><a href="/home/index">@Resources.Resource.home</a></li>
                            <li><a href="mens.html">@Resources.Resource.venders</a></li>
                            <li><a href="womens.html">@Resources.Resource.categories</a></li>
                        </ul>
                    </div>

                    <div class="col-md-4 sign-gd-two">
                        <h4>@Resources.Resource.contactInfo</h4>
                        <ul>
                            <li><i class="glyphicon glyphicon-map-marker" aria-hidden="true"></i>@Resources.Resource.adress : @Resources.Resource.adressActual</li>
                            <li><i class="glyphicon glyphicon-envelope" aria-hidden="true"></i>@Resources.Resource.email : <a href="mailto:@Resources.Resource.emailActual">@Resources.Resource.emailActual</a></li>
                            <li><i class="glyphicon glyphicon-earphone" aria-hidden="true"></i>@Resources.Resource.phone : @Resources.Resource.phoneActual</li>
                        </ul>
                    </div>
                    
                    <div class="clearfix"></div>
                </div>
            </div>
            <div class="clearfix"></div>
            <p class="copy-right">&copy 2016 Smart Shop. All rights reserved | Design by <a href="http://w3layouts.com/">W3layouts</a></p>
        </div>
    </div>
    <!-- //footer -->
    <!-- login -->
    <div class="modal fade" id="myModal4" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content modal-info">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                </div>
                <div class="modal-body modal-spa">
                    <div class="login-grids">
                        <div class="login">
                            <div class="login-bottom">
                                <h3>@Resources.Resource.seengUpFree</h3>
                                <form>
                                    <div class="sign-up">
                                        <h4>@Resources.Resource.email :</h4>
                                        <input type="text" value="Type here" onfocus="this.value = '';" onblur="    if (this.value == '') { this.value = '@Resources.Resource.email'; }" required="">
                                    </div>
                                    <div class="sign-up">
                                        <h4>@Resources.Resource.password :</h4>
                                        <input type="password" value="Password" onfocus="this.value = '';" onblur="    if (this.value == '') { this.value = '@Resources.Resource.password'; }" required="">

                                    </div>
                                    <div class="sign-up">
                                        <h4>@Resources.Resource.reTypePassword :</h4>
                                        <input type="password" value="Password" onfocus="this.value = '';" onblur="    if (this.value == '') { this.value = '@Resources.Resource.password'; }" required="">

                                    </div>
                                    <div class="sign-up">
                                        <input type="submit" value="@Resources.Resource.registerNow">
                                    </div>

                                </form>
                            </div>
                            <div class="login-right">
                                <h3>@Resources.Resource.signInYourAccount</h3>
                                <form>
                                    <div class="sign-in">
                                        <h4>@Resources.Resource.signInYourAccount :</h4>
                                        <input type="text" value="Type here" onfocus="this.value = '';" onblur="    if (this.value == '') { this.value = '@Resources.Resource.email'; }" required="">
                                    </div>
                                    <div class="sign-in">
                                        <h4>@Resources.Resource.password :</h4>
                                        <input type="password" value="@Resources.Resource.password" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = '@Resources.Resource.password';}" required="">
                                        <a href="#">@Resources.Resource.forgotPassword ?</a>
                                    </div>
                                    <div class="single-bottom">
                                        <input type="checkbox" id="brand" value="">
                                        <label for="brand"><span></span>@Resources.Resource.rememberMe.</label>
                                    </div>
                                    <div class="sign-in">
                                        <input type="submit" value="@Resources.Resource.SIGNIN">
                                    </div>
                                </form>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <p>@Resources.Resource.Bylogginginyouagreetoour <a href="#">@Resources.Resource.TermsAndConditions</a>@Resources.Resource.and<a href="#">@Resources.Resource.privacyPolicy</a></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- //login -->
</body>
</html>
